{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "rootDir": "src",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ESNext"
  },
  "exclude": ["node_modules"]
}
